<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="1.0"
    STATUS="ALPHA"
  
    COMPATIBILITY="ALL"
    AUTHOR="Prabhakaran"
    RELEASE_DATE="20181024A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>
  
  <FAMILIES>
    <FAMILY>EthereumContract CRUD</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>
    

    <!-- <PARAMETER NAME="GETH_SERVER" FIELD="TEXT"
			 REQUIRED="false" NUM_ROW="50">
			<DEFAULT>"http://192.168.1.14:8545"</DEFAULT>
		</PARAMETER> -->

    <PARAMETER NAME="GETH_SERVER" FIELD="TEXT"
			 REQUIRED="false" NUM_ROW="10">
			<DEFAULT>"Your geth server ip"</DEFAULT>
		</PARAMETER>

    <PARAMETER NAME="WALLET_LOCATION" FIELD="TEXT"
			 REQUIRED="true" NUM_ROW="11">
			<DEFAULT>"Your wallet location"</DEFAULT>
		</PARAMETER>

    <PARAMETER NAME="WALLET_PASSWORD" FIELD="PASSWORD"
			 REQUIRED="true" NUM_ROW="12">
			<DEFAULT>"Wallet password"</DEFAULT>
		</PARAMETER>

    <PARAMETER NAME="CONTRACT_ADDRESS" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="13">
			<DEFAULT>"Contract address"</DEFAULT>
		</PARAMETER>

   

    <PARAMETER NAME="CREATE_CONTRACT" FIELD="RADIO" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>


    <PARAMETER NAME="CREATE_CUSTOMER" FIELD="RADIO" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>    
	
	  <PARAMETER NAME="CREATE_CUSTOMER_ID" FIELD="TEXT"
			SHOW_IF="CREATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="21">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	
	  <PARAMETER NAME="CREATE_CUSTOMER_NAME" FIELD="TEXT"
			SHOW_IF="CREATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="21">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>		
		
	  <PARAMETER NAME="CREATE_CUSTOMER_ADDRESS" FIELD="TEXT"
			SHOW_IF="CREATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="23">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
	  <PARAMETER NAME="CREATE_CUSTOMER_DOB" FIELD="TEXT"
			SHOW_IF="CREATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="22">
			<DEFAULT>"Format should dd/MM/yyyy"</DEFAULT>
		</PARAMETER>

    <PARAMETER NAME="CREATE_CUSTOMER_AGE" FIELD="INTEGER"
			SHOW_IF="CREATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="22">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
			
	
	
	  <PARAMETER NAME="GET_CUSTOMER" FIELD="RADIO" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
	  <PARAMETER NAME="GET_CUSTOMER_ID" FIELD="TEXT"
			SHOW_IF="GET_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="21">
			<DEFAULT>"101"</DEFAULT>
		</PARAMETER>	


    <PARAMETER NAME="UPDATE_CUSTOMER" FIELD="RADIO" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>	
	
	  <PARAMETER NAME="UPDATE_CUSTOMER_ID" FIELD="TEXT"
			SHOW_IF="UPDATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="21">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
	  <PARAMETER NAME="CUSTOMER_NEW_ADDRESS" FIELD="TEXT"
			SHOW_IF="UPDATE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="22">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>	
		

    <PARAMETER NAME="DELETE_CUSTOMER" FIELD="RADIO" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
	  <PARAMETER NAME="DELETE_CUSTOMER_ID" FIELD="TEXT"
			SHOW_IF="DELETE_CUSTOMER == 'true'" REQUIRED="true" NUM_ROW="22">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>	
		
	
	
	


  </PARAMETERS>
  
  <CODEGENERATION>
  <IMPORTS>
<IMPORT MODULE="com.sample.smartcontract.jar" NAME="smartcontract" REQUIRED="true"/> 

</IMPORTS>
  </CODEGENERATION>
    
  <RETURNS/>
  
</COMPONENT>